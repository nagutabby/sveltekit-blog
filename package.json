    "postcss": "^8.5.3",
    "prisma": "^6.6.0",
    "sass": "^1.86.3",
    "sharp": "^0.34.2",
    "shiki": "^3.3.0",
    "svelte": "^5.25.9",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.1.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.4.1",
    "vitest": "^3.1.1",
    "wrangler": "^4.9.1"
  },
  "type": "module",
  "packageManager": "pnpm@10.11.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
